
let rec remainder(x,y) = 
	x >= y -> remainder(x - y,y)
	| x
in
let rec reverse(num1,num2) = 
	num1 eq 0 -> num2
	| reverse(num1/10,remainder(num1,10)+num2*10)
in
let isPalindrome(num3) = 
	num3 eq reverse(num3,0) -> true
	| false
in
let palTuple = nil
in
let rec printPal(start,end,palTuple) = 
	start > end -> palTuple
	| isPalindrome(start) -> printPal(start+1,end,palTuple aug start)
	| printPal(start+1,end,palTuple)
in
Print(printPal(100,200,palTuple))
